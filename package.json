{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"npm run start\"",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
    "typecheck": "tsc",
    "pages:deploy": "pnpm run build && wrangler pages deploy ./public",
    "fmt": "prettier -c ./app/**/*.{ts,tsx}",
    "fmt:fix": "prettier -w ./app/**/*.{ts,tsx}",
    "lint": "eslint ./app/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix ./app/**/*.{ts,tsx}"
  },
  "dependencies": {
    "@ark-ui/react": "^0.10.0",
    "@remix-run/cloudflare": "^1.16.0",
    "@remix-run/cloudflare-pages": "^1.16.0",
    "@remix-run/css-bundle": "^1.16.0",
    "@remix-run/react": "^1.16.0",
    "@vanilla-extract/css": "^1.12.0",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.19.0",
    "@remix-run/dev": "^1.16.0",
    "@remix-run/eslint-config": "^1.16.0",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.1",
    "prettier": "^3.0.1",
    "typescript": "^5.0.4",
    "wrangler": "^3.1.1"
  },
  "engines": {
    "node": ">=16.13.0",
    "pnpm": ">=8.6.0",
    "npm": "please use pnpm!",
    "yarn": "please use pnpm!"
  }
}
